//Q.8: Write a program that takes a list of numbers as input 
// and print the numbers greater than 5 using for a loop and if-else condition


void main() {
  List<int> numbers = [3, 7, 2, 9, 4, 6, 8];

  print('Numbers greater than 5:');
  for (int number in numbers) {
    if (number > 5) {
      print(number);
    }
  }
}